{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["state", "year", "source_url", "checked_at", "effective_date"],
  "properties": {
    "state": { "type": "string", "minLength": 2, "maxLength": 2 },
    "year": { "type": "integer" },
    "acs_median_income": { "type": "number" },
    "bea_rpp": { "type": "number" },
    "bls_unemp_rate": { "type": "number" },
    "source_url": { "type": "string", "minLength": 8 },
    "checked_at": { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}" },
    "effective_date": { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}" },
    "notes": { "type": "string" }
  },
  "additionalProperties": true
}
