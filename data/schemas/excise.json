{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["state", "year"],
  "properties": {
    "state": { "type": "string", "minLength": 2, "maxLength": 2 },
    "year": { "type": "integer" },
    "cigarette_per_pack": { "type": "number" },
    "alcohol_per_unit": { "type": "number" },
    "beer_per_gal": { "type": "number" }
  },
  "additionalProperties": true
}
