{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": ["state", "year", "source_url", "checked_at", "effective_date"],
  "properties": {
    "state": { "type": "string", "minLength": 2, "maxLength": 2 },
    "year": { "type": "integer" },
    "cigarette_per_pack": { "type": "number", "minimum": 0 },
    "alcohol_per_unit": { "type": "number", "minimum": 0 },
    "beer_per_gal": { "type": "number", "minimum": 0 },
    "cigarette_unit": { "type": "string", "enum": ["per_pack"] },
    "alcohol_unit": { "type": "string", "enum": ["per_unit", "per_gallon"] },
    "source_url": { "type": "string", "minLength": 8 },
    "checked_at": { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}" },
    "effective_date": { "type": "string", "pattern": "^\\d{4}-\\d{2}-\\d{2}" },
    "notes": { "type": "string" }
  },
  "additionalProperties": true
}
